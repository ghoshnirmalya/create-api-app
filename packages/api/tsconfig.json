{
  "compilerOptions": {
    "alwaysStrict": true,
    "allowUnreachableCode": false,
    "noImplicitAny": true,
    "strictNullChecks": true,
    "noImplicitReturns": true,
    "noUncheckedIndexedAccess": true,
    "noUnusedLocals": true,
    "esModuleInterop": true
  },
  "include": [
    "./**/*.ts",
  ],
  "exclude": [
    "node_modules/**/*"
  ]
}
